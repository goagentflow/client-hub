{
  "name": "@agentflow/middleware",
  "version": "0.1.0",
  "description": "AgentFlow middleware - stateless API layer for M365 integration",
  "type": "module",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "prisma generate && tsup",
    "db:migrate": "prisma migrate deploy",
    "db:generate": "prisma generate",
    "start": "node dist/server.js",
    "test": "vitest",
    "test:run": "vitest run",
    "lint": "eslint src/",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "agentflow",
    "m365",
    "sharepoint",
    "middleware"
  ],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.24.0",
  "pnpm": {
    "onlyBuiltDependencies": [
      "@prisma/engines",
      "prisma",
      "esbuild"
    ]
  },
  "dependencies": {
    "@azure/msal-node": "^3.8.3",
    "@microsoft/microsoft-graph-client": "^3.0.7",
    "@prisma/client": "^6.19.2",
    "@supabase/supabase-js": "^2.97.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "express-rate-limit": "^8.2.1",
    "helmet": "^8.1.0",
    "jose": "^6.1.3",
    "pino": "^10.1.0",
    "pino-http": "^11.0.0",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/express-serve-static-core": "^5.1.1",
    "@types/node": "^24.10.1",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.48.0",
    "@typescript-eslint/parser": "^8.48.0",
    "eslint": "^9.39.1",
    "pino-pretty": "^13.1.3",
    "prisma": "^6.19.2",
    "supertest": "^7.2.2",
    "tsup": "^8.5.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.14"
  }
}
